{
  "settings": {
    "react": {
      "createClass": "createReactClass",
      "pragma": "React",
    },
    "propWrapperFunctions": [ "forbidExtraProps" ]
  },
   "env": {
       "browser": true,
       "es6": true,
       "node": true
   },
   "extends": [
   "eslint:recommended",
   "plugin:react/recommended",
   ],
   "parserOptions": {
       "ecmaFeatures": {
           "jsx": true
       },
       "ecmaVersion": 2018,
       "sourceType": "module"
   },
   "plugins": [
       "react"
   ],
   "rules": {
      "react/prop-types": [0],
       "indent": [
           "error",
           2
       ],
       "linebreak-style": [
           "error",
           "unix"
       ],
       "comma-dangle": 0,
       "no-console": 0,
       "quotes": [
            0,
            "double",
            "avoid-escape"
        ],
       # "quotes": [
       #     "error",
       #     "double",
       # ],
       "semi": [
           "error",
           "always"
       ],
       "react/jsx-uses-react": "error",
       "react/jsx-uses-vars": "error",
       "closingSlash": "never",
       "beforeSelfClosing": "always",
       "afterOpening": "never",
       "beforeClosing": "allow",
      "ignoreEOLComments": true,
      "exceptions": {
        "Property": true
        }
   }
}
